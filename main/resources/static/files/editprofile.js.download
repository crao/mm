    if(val == 1){
        dgebid(Habitname+'2').checked=false;
        dgebid(Habitname+'3').checked=false;
    } else if(val == 2){
        dgebid(Habitname+'1').checked=false;
        dgebid(Habitname+'3').checked=false;
    } else if(val == 3){
        dgebid(Habitname+'1').checked=false;
        dgebid(Habitname+'2').checked=false;
    }
}

