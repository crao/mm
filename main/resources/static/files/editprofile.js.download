function editftocms()
{    
    if($("#FEET").val()!=0)
    {
        var feet_val=$("#FEET").val();
        if(feet_val.indexOf("-")!=-1)
        {
            var htarr=feet_val.split("-");
            var incms=parseInt(htarr[0])*30.48+parseInt(htarr[1])*2.54;
            $("#CMS").val(Math.round(incms));
        }
        else
        {
            var incms=parseInt(feet_val)*30.48;
            $("#CMS").val(Math.round(incms));
        }
        $("#INCMS").val("0");
    }
}

function editcmstof()
{
    if($("#CMS").val()!=0)
    {
        var cms_val=$("#CMS").val();
        var feet=parseInt(cms_val)/30.48;
        feet=feet.toString();
        var f1=feet.split(".");
        var f2="0."+f1[1];
        var inc=Math.round(f2*100)
        inc=Math.round(inc*0.12)
        if (inc>=12)
        {
            inc=inc-12;
            f1[0]=parseInt(f1[0])+1;
        }
        if(inc==0)
        {
            $("#FEET").val(f1[0]);
        }else{
            $("#FEET").val(f1[0]+"-"+inc);
        }

        $("#INCMS").val("1");
    }
}


