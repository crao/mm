function getAutolist(){ 
    var resval = new Array();
    $('#autocmplytvalid').val("0");
    $('#COLLEGE').keyup(function(e){

 var myhomeinst = $("#myhomeinst").val();
 if(myhomeinst == 1)
 {
     myhomeinstcount = 4;
 } else {
     myhomeinstcount = 10;
 }
        var keynum;
        if(window.e){ // IE
            keynum = e.keyCode;
        }else{
            if(e.which) // Netscape/Firefox/Opera
                keynum = e.which;
            else  
                keynum = e.keyCode;
        }

        if(!((keynum <=123 && keynum >=112)||(keynum <=40 && keynum >=37)||(keynum <=18 && keynum >=16)))
        {

            var serchStr = $.trim($('#COLLEGE').val());

            $('#autolist').hide();
            if(serchStr.length>3){
                clearTimeout(typingTimeout);
                typingTimeout = setTimeout( function()
                {
                    $('.college_ajx_img').show();
                    if(ajxf == 1)
                    {
                        Axhr.abort();
                    }
                    Axhr = $.ajax({
                        type: 'GET',
                        url: '//'+curDomain+'/profiledetail/institutionautocomplete.php',
                        data: {
                            term: 	serchStr,
                            myhomeinstcount : myhomeinstcount
                        },
                        beforeSend:function(){
                            ajxf =1;
                        }, 
                        timeout: 30000,
                        success : function (res){	
                            if(res !="")
                            {
                                ajxprog=1;
                                resval  =	res.split("~");
                                if(resval[0] !="nomatch" && resval[0] !="")
                                {
                                    $('#autolist').html(resval[0]);
                                    $('#autolist').show();
                                    resarray = resval[1].split("^");
                                    var Arrt = $.inArray(serchStr,resarray);
                                    if(Arrt != -1)
                                    {
                                        $('#autocmplytvalid').val("1");
                                    }
                                    else
                                    {
                                        $('#autocmplytvalid').val("0");
                                    }
                                }
                                else{
                                    $('#autocmplytvalid').val("0");
                                    $('#autolist').hide();
                                }
                                $('.college_ajx_img').hide();
                            }
                        },
                        complete: function (jqXHR,textStatus) { 
                            if(textStatus == 'timeout'){
                                Axhr.abort();
                            }
                            ajxf =0;
                        }
					
					
					
                    });
                }, 500);
            }
            else{
		
                $('#autolist').hide();
                $('.college_ajx_img').hide();
                clearTimeout(typingTimeout);
		
                if(ajxprog == 1)
                {
                    Axhr.abort();
                }
		
                $('#autocmplytvalid').val("0");
		
            }
        }
	
    });
	

}

